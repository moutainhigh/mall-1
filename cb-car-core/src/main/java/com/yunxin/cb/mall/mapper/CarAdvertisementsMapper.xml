		</if>
		<if test="updateTime!=null ">
			update_time=#{updateTime,jdbcType = TIMESTAMP},
		</if>
		<if test="isDelete!=null ">
			is_delete=#{isDelete,jdbcType = INTEGER},
		</if>
		<if test="operateId!=null ">
			operate_id=#{operateId,jdbcType = INTEGER},
		</if>
	</set>
	<where>
		AND id=#{id}
	</where>
</update>

<delete id="delete">
	delete from CAR_ADVERTISEMENTS 
	<where>
		id=#{0}
	</where>
</delete>

<select id="get" resultMap="CarAdvertisements">
	select <include refid="AllColumnlist"/> from CAR_ADVERTISEMENTS
	<where>
		id=#{0}
	</where>
</select>

<select id="getByIds" resultMap="CarAdvertisements">
	select <include refid="AllColumnlist"/> from CAR_ADVERTISEMENTS
	<where>
		id in <foreach item="item" index="index" collection="array" open="(" separator="," close=")">#{item}</foreach>
	</where>
</select>

<select id="count" parameterType="Query" resultType="Long">
	select count(id) from CAR_ADVERTISEMENTS 
	<where>
	<include refid="listCommonWhere"/>
	</where>
</select>

<select id="queryAll" parameterType="Query" resultMap="CarAdvertisements">
	select <include refid="CommonColumnlist"/> from CAR_ADVERTISEMENTS 
	<where>
