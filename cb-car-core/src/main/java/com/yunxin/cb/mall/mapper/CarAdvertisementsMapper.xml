	</where>
</select>

<select id="pageList" parameterType="Query" resultMap="CarAdvertisements">
	select <include refid="CommonColumnlist"/> from CAR_ADVERTISEMENTS
	<where>
	 <include refid="listCommonWhere"/>
	</where>
	<![CDATA[LIMIT #{rowIndex},#{pageSize} ]]>
</select>


<sql id="listCommonWhere">
    <if test="q==null"><!-- 无传入参数时默认查未删除的 -->
       AND is_delete=0
    </if>
	<if test="q!=null">
		<if test="q.id!=null ">
			AND id=#{q.id}
		</if>
		<if test="q.title!=null and q.title!=''">
			AND title=#{q.title}
