<insert id="add" parameterType="CarAdvertisements" useGeneratedKeys="true" keyProperty="id">
	insert into CAR_ADVERTISEMENTS(
		<if test="id!=null">
		id,
		</if>
		title,
		adver_type,
		position,
		state_time,
		end_time,
		enabled,
		jump_type,
		jump_url,
		brand_id,
		sys_id,
		content,
		create_time,
		update_time,
		is_delete,
		operate_id
	) values (
		<if test="id!=null">
		#{id,jdbcType = INTEGER},
		</if>
		#{title,jdbcType = VARCHAR},
		#{adverType,jdbcType = VARCHAR},
		#{position,jdbcType = VARCHAR},
		#{stateTime,jdbcType = TIMESTAMP},
		#{endTime,jdbcType = TIMESTAMP},
		#{enabled,jdbcType = BIT},
		#{jumpType,jdbcType = INTEGER},
		#{jumpUrl,jdbcType = VARCHAR},
		#{brandId,jdbcType = INTEGER},
		#{sysId,jdbcType = INTEGER},
		#{content,jdbcType = LONGTEXT},
		#{createTime,jdbcType = TIMESTAMP},
		#{updateTime,jdbcType = TIMESTAMP},
		#{isDelete,jdbcType = INTEGER},
		#{operateId,jdbcType = INTEGER}
	)
</insert>

<update id="update" parameterType="CarAdvertisements">
	update CAR_ADVERTISEMENTS 
	<set>
		<if test="title!=null and title!=''">
